@@
@Name: Correct logical expressions 2
@Expect: "b" "e"

CREATE x y z

IF x == 5 OR y <> 6 + x AND 5 * z <= 6
    PRINT "a"
;;
SET x TO 5
IF x == 5 OR y <> 6 + x AND 5 * z <= 6
    PRINT "b"
;;
SET x TO 1
SET y TO 7
SET z TO 2
IF x == 5 OR y <> 6 + x AND 5 * z <= 6
    PRINT "c"
;;
SET y TO 0
IF x == 5 OR y <> 6 + x AND 5 * z <= 6
    PRINT "d"
;;
SET z TO -10
IF x == 5 OR y <> 6 + x AND 5 * z <= 6
    PRINT "e"
;;

IF (x == 5) OR (y <> 6 + x) AND (5 * z <= 6)
;;


IF (x == 5 OR y <> (6 + x)) AND !(5 * z <= 6)
;;